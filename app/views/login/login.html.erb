<h1>Logue-se</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(:login, url: login_auth_path) do |f| %>
      <%= f.label :email %>
      <%= f.email_field :email %>

      <%= f.label :password %>
      <%= f.password_field :password %>
      
      <%= f.submit "Login", class: "btn" %>
    <% end %>
  </div>
</div>